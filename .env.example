# Environment Configuration for Parable Interview
# Copy this file to .env and fill in your actual API keys
# DO NOT commit .env to version control

# ============================================
# OpenAI API Configuration
# ============================================
# Get your API key from: https://platform.openai.com/api-keys
OPENAI_API_KEY=sk-proj-...

# Optional: Specify which model to use
OPENAI_MODEL=gpt-4-turbo-preview

# Optional: Control API costs
OPENAI_MAX_TOKENS=4096

# ============================================
# LangChain Configuration
# ============================================
# For LangChain tracing (optional)
# LANGCHAIN_TRACING_V2=true
# LANGCHAIN_API_KEY=...
# LANGCHAIN_PROJECT=parable-interview

# ============================================
# Data Configuration
# ============================================
# Path to your data files (if different from default)
DATA_DIR=./data
RAW_DATA_FILE=./data/raw_fixture.json
PARSED_DATA_FILE=./data/parsed_data.json

# ============================================
# Analysis Configuration
# ============================================
# Adjust these for your analysis preferences
TIMEZONE=America/Los_Angeles
DATE_FORMAT=%Y-%m-%d %H:%M:%S
